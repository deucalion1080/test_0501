    "#買機車 機車約30000元 將每月薪水存入 足夠後顯示跳出訊息通知\n",
    "salary = 0\n",
    "month = 0\n",
    "while(salary < 30000):\n",
    "    month += 1\n",
    "    input_value = int(input(f\"請輸入第{month}月份存入金額:\"))\n",
    "    salary += input_value\n",
    "\n",
    "print(f\"恭喜!已經存了{month}個月,總共存了{salary}元\")"
   ]
  }
